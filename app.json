{
  "name": "Qual Pro",
  "description": "This app does one thing, and does it well.",
  "keywords": [
    "productivity"
  ],
  "repository": "https://github.com/rhinobuccaneers/qualpro.git",
  "success_url": "/",
  "env": {
    "WEB_CONCURRENCY": {
      "required": false,
      "description": "The number of processes to run."
    },
    "NODE_ENV": {
      "required": true,
      "description": "Node environment",
      "value": "development"
    },
    "PORT": {
      "required": false,
      "description": "Node port"
    },
    "REDIS_URL": {
      "required": false,
      "description": "Redis uri",
      "value": "Not specified."
    },
    "MONGODB_URI": {
      "required": false,
      "description": "MongoDB URI",
      "value": "Not specified."
    },
    "SENDGRID_APIKEY": {
      "required": true,
      "description": "SendGrid API key",
      "value": "Not specified."
    },
    "SMS_ACCOUNT_SID": {
      "required": true,
      "description": "Twilio account SID",
      "value": "Not specified."
    },
    "SMS_AUTH_TOKEN": {
      "required": true,
      "description": "Twilio authentication token",
      "value": "Not specified."
    },
    "SMS_NUMBER": {
      "required": true,
      "description": "Twilio sms number",
      "value": "Not specified."
    },
    "AWS_S3_BUCKET": {
      "required": true,
      "description": "Name of bucket on S3",
      "value": "Not specified."
    },
    "AWS_S3_REGION": {
      "required": true,
      "description": "S3 region",
      "value": "Not specified."
    },
    "AWS_ACCESS_KEY_ID": {
      "required": true,
      "description": "AWS access key ID",
      "value": "Not specified."
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true,
      "description": "AWS secret access key",
      "value": "Not specified."
    },
    "FCM_API_KEY": {
      "required": true,
      "description": "Firebase Cloud Messaging API key",
      "value": "Not specified."
    },
    "PUBNUB_PUBLISH_KEY": {
      "required": true,
      "description": "PubNub publish key",
      "value": "Not specified."
    },
    "PUBNUB_SUBSCRIBE_KEY": {
      "required": true,
      "description": "PubNub subscribe key",
      "value": "Not specified."
    },
    "SCHEDULER_HOST": {
      "required": true,
      "description": "URL to Abstract Scheduler",
      "value": "Not specified."
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "Standard-2X"
    }
  },
  "addons": [
    {
      "plan": "papertrail:forsta"
    },
    {
      "plan": "heroku-redis:premium-1"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/rhinobuccaneers/heroku-buildpack-multi"
    }
  ]
}